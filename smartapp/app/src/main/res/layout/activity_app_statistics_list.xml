<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_app_statistics_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.longfeizeng.smartapp.view.AppStatisticsList">



    <ListView
        android:id="@+id/AppStatisticsList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonbarList3" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="145px"
        android:orientation="horizontal"
        android:id="@+id/buttonbarList3">
        
        <Button
            android:text="NOW"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:id="@+id/tineline" />

        <Button
            android:text="DAY"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:id="@+id/daybuttonlist3"
            />

        <Button
            android:text="WEEK"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:id="@+id/weekbuttonlist3"
            />

        <Button
            android:text="MONTH"
            android:layout_width="0px"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:id="@+id/monthbuttonlist3"
            />
        
    </LinearLayout>

</RelativeLayout>
